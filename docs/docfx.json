{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "bin/Debug/**/LemonUI.*.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/**.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "obj/**"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default"
    ],
    "postProcessors": []
  }
}
